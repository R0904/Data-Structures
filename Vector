#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main()
 {
     int a,b,c,d,e;
     cin>>a;
     cin>>b;
     vector<vector<int>> v(a);
     for(int i=0;i<a;i++)
     {
         cin>>c;
         while(c--)
         {
             int x;
             cin>>x;
             v[i].push_back(x);
         }
     }
     while(b--)
     {
         int i,j;
         cin>>i>>j;
         cout<<v[i][j]<<endl;
     }
     
 

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    return 0;
}
